{
  "name": "nimbus-cli",
  "description": "An experimental cli for creating nimbus experiments",
  "version": "0.1.4",
  "author": "Kate Hudson @k88hudson",
  "bin": {
    "nimbus-cli": "./bin/run"
  },
  "bugs": "https://github.com/k88hudson/nimbus-cli/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "inquirer": "^7.3.3",
    "node-fetch": "^2.6.1",
    "open": "^7.3.0",
    "tslib": "^1.13.0",
    "unique-names-generator": "^4.3.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.7",
    "@types/chai": "^4.2.13",
    "@types/inquirer": "^7.3.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.37",
    "@types/node-fetch": "^2.5.7",
    "@types/open": "^6.2.1",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/k88hudson/nimbus-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MPL-2.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "nimbus-cli",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "k88hudson/nimbus-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
